{
  "name": "linter-drupalcs",
  "version": "1.1.1",
  "main": "./lib/main",
  "description": "Lint Drupal on the fly, using phpcs and Drupal standards",
  "repository": "https://github.com/haringsrob/linter-drupalcs",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-linter": "^3.0.0",
    "atom-package-deps": "^2.1.3",
    "minimatch": "^3.0.0"
  },
  "devDependencies": {
    "coffeelint": "^1.12.1"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
